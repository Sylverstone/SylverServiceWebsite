{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "rootDir": ".",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": false,
    "forceConsistentCasingInFileNames": true
    
  },
  "include": ["**/*"],
  "exclude": ["node_modules", "dist"]
}